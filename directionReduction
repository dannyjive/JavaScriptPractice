  const opposite = {
    NORTH: "SOUTH",
    SOUTH: "NORTH",
    EAST: "WEST",
    WEST: "EAST"
  };

function dirReduc(arr){
  
  const stack = [];
  
  for (let dir of arr){
    if (stack.length > 0 && opposite[dir] === stack[stack.length - 1]){
      stack.pop()
    } else {
      stack.push(dir);
    }
  }
  
  return stack;
}